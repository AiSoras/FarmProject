<web-app>
  <servlet>
   <servlet-name>ObjectService</servlet-name>
   <servlet-class>com.caucho.hessian.server.HessianServlet</servlet-class>
    <init-param>
      <param-name>home-class</param-name>
      <param-value>implementations.ObjectServiceImplementation</param-value>
    </init-param>
    <init-param>
      <param-name>home-api</param-name>
      <param-value>api.ObjectService</param-value>
    </init-param>
  </servlet>
  <servlet>
   <servlet-name>AccountService</servlet-name>
   <servlet-class>com.caucho.hessian.server.HessianServlet</servlet-class>
    <init-param>
      <param-name>home-class</param-name>
      <param-value>implementations.AccountServiceImplementation</param-value>
    </init-param>
    <init-param>
      <param-name>home-api</param-name>
      <param-value>api.AccountService</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <url-pattern>/ObjectService</url-pattern>
    <servlet-name>ObjectService</servlet-name>
  </servlet-mapping>
  <servlet-mapping>
    <url-pattern>/AccountService</url-pattern>
    <servlet-name>AccountService</servlet-name>
  </servlet-mapping>
</web-app>